  <!DOCTYPE html>
  <!-- ABOUT -->
  <html lang="en">
    <head>
      <link rel="manifest" href="manifest.json">
      <link rel="icon" href="" type="image/x-icon">    
      <link rel="shortcut icon" href="" type="image/x-icon">
      <meta name="description" content="Bits Workshop Order System"/>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="../../materialize.min.css"  media="screen,projection"/>
      <link type="text/css" rel="stylesheet" href="../../userinfo-style.css"/>
      <meta name="theme-color" content="black">
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <script async type="text/javascript" src="../../min.js"></script>
      <title>Workshop | Bits Pilani</title>
    </head>

    <body>
      <!--Import jQuery before materialize.js-->
      <script async type="text/javascript" src="../../jquery214.js"></script>
      <script async type="text/javascript" src="../../materialize.min.js"></script>
      <script async type="text/javascript" src="../../dashboard-action.js"></script>
      <!-- NAVIGATION -->
      <% include partials/downAdmL2.ejs %>
      <!-- EIGHTY FOUR -->
      <div class="eightyfour grey lighten-2">
        <div id="allusers" class="eightyfourin">
          <table class="centered table white">
            <thead>
              <tr>
                  <th>Order Name</th>
                  <th>Order Id</th>
                  <th>Item Qty</th>
                  <th>Order Status</th>
                  <th>Configure Order</th>
              </tr>
            </thead>

            <tbody>
              <% for(var i=0; i < data.orders.length; i++) { %>
                <tr>
                  <td><%= data.orders[i].ordername %></td>
                  <td><%= data.orders[i].orderid %></td>
                  <td><%= data.orders[i].orderqty %></td>
                  <td><%= data.orders[i].orderstatus %></td>
                  <td><a href="../order/<%= data._id %>/<%= data.orders[i].orderid %>">Click Here</a></td>
                 </tr>
              <% } %>
            </tbody>

            <tfoot>
              <tr>
                  <th>Customer Name: <%= data.username %></th>
                  <th>Customer Id: <%= data._id %></th>
                  <th>Number of Orders: <%= data.orders.length %></th>
              </tr>
            </tfoot>
          </table>
        </div>
      </div>
      <!-- FOOTER -->
      <% include partials/footer.ejs %>
    </body>
  </html>
