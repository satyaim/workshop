  <!DOCTYPE html>
  <!-- ABOUT -->
  <html lang="en">
    <head>
      <link rel="manifest" href="manifest.json">
      <link rel="icon" href="" type="image/x-icon">    
      <link rel="shortcut icon" href="" type="image/x-icon">
      <meta name="description" content="Bits Workshop Order System"/>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="materialize.min.css"  media="screen,projection"/>
      <link type="text/css" rel="stylesheet" href="myorders-style.css"/>
      <meta name="theme-color" content="black">
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <script type="text/javascript" src="min.js"></script>
      <title>Connect</title>
    </head>

    <body>
      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="jquery214.js"></script>
      <script type="text/javascript" src="materialize.min.js"></script>
      <script type="text/javascript" src="customer-action.js"></script>
      <!-- NAVIGATION -->
      <% include partials/footer.ejs %>
      <!-- EIGHTY FOUR -->
      <div class="eightyfour grey lighten-2">
        <div id="myorders" class="eightyfourin">
          <table class="centered table white">
            <thead>
              <tr>
                  <th>Order Id</th>
                  <th>Item Name</th>
                  <th>Item Quantity</th>
                  <th>Order Status</th>
                  <th>Order Info</th>
              </tr>
            </thead>

            <tbody>
              <% for(var i=0; i < orders.length; i++) { %>
                <tr>
                  <td><%= orders[i].orderid %></td>
                  <td><%= orders[i].ordername %></td>
                  <td><%= orders[i].orderqty %></td>
                  <td><%= orders[i].orderstatus %></td>
                  <td><a href="./myorders/<%= orders[i].orderid %>">Click Here</a></td>
                 </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
      <!-- FOOTER -->
      <% include partials/footer.ejs %>
    </body>
  </html>
