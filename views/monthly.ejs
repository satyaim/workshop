  <!DOCTYPE html>
  <!-- ABOUT -->
  <html lang="en">
    <head>
      <link rel="manifest" href="manifest.json">
      <link rel="icon" href="" type="image/x-icon">    
      <link rel="shortcut icon" href="" type="image/x-icon">
      <meta name="description" content="Bits Workshop Order System"/>
      <!--Import Google Icon Font-->
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <!--Import materialize.css-->
      <link type="text/css" rel="stylesheet" href="../materialize.min.css"  media="screen,projection"/>
      <link type="text/css" rel="stylesheet" href="../allusers-style.css"/>
      <meta name="theme-color" content="black">
      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <script type="text/javascript" src="../min.js"></script>
      <title>Workshop | Bits Pilani</title>
    </head>

    <body>
      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="../jquery214.js"></script>
      <script type="text/javascript" src="../materialize.min.js"></script>
      <script type="text/javascript" src="../dashboard-action.js"></script>
      <!-- NAVIGATION -->
      <% include partials/downAdmL1.ejs %>
      <!-- EIGHTY FOUR -->
      <div class="eightyfour grey lighten-2">
        <table class="centered table white responsive-table">
            <thead>
              <tr>
                  <th>User Name</th>
                  <% date= new Date(); rate= new Date(); for(var i=0; i < 10; i++){ %>
                  <th><%= new Date(rate.setDate(date.getDate() - i)).toLocaleDateString('en-GB') %></th>
                  <% } %>
              </tr>
            </thead>

            <tbody>
              <% for(var i=0; i < data.length; i++) { if(data[i].type=="worker"){ %>
                <tr>
                  <td><%= data[i].username%></td>
                  <% pause=0; last=1; while(pause==0){
                      %>
                  data[i].logout[data[i].logout.length-i]
                 </tr>
              <% }} %>
            </tbody>
          </table>
      </div>
      <!-- FOOTER -->
      <% include partials/footer.ejs %>
    </body>
  </html>
